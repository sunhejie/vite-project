{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build ",
    "build:test": "cross-env NODE_ENV=online_test vite build --mode online_test ",
    "preview": "vite preview",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": "prettier --write"
  },
  "dependencies": {
    "ant-design-vue": "^2.2.8",
    "axios": "^0.26.0",
    "pinia": "^2.0.0-rc.10",
    "vue": "^3.2.25",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@eslint/create-config": "^0.1.2",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/compiler-sfc": "^3.2.31",
    "babel-core": "7.0.0-bridge.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.6",
    "postcss-nesting": "^10.1.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "ts-node": "^10.5.0",
    "vite": "^2.8.0",
    "vite-plugin-windicss": "^1.7.0",
    "vue-template-compiler": "^2.6.14",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "vite": {
    "optimizeDeps": {
      "include": [
        "vue",
        "vue-router"
      ]
    }
  }
}
